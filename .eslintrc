{
    "root": true,
    "extends": ["ao", "plugin:prettier/recommended"],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "no-use-before-define": "off",
        "no-console": "off",
        "consistent-this": "off",
        "no-extend-native": "off",
        "max-len": "off",
        "no-process-env": "off"
    }
}
